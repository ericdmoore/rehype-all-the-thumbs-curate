{
  "name": "rehype-all-the-thumbs-curate",
  "version": "0.2.0",
  "description": "Supporting rehype-all-the-thumbs by finding elements to be processed",
  "main": "src/index.js",
  "files": [
    "dist/umd/index.js",
    "dist/index.min.js"
  ],
  "scripts": {
    "test": "nyc tape tests/**/*.test.js | tap-nyc",
    "test:NYC": "nyc npm test | tap-nyc",
    "docs": "gulp doc && npm run openDocs",
    "_docs": "jsdoc src/index.js -c .jsdoc.json",
    "openDocs":"open file:///Users/ericmoore/Web/rehype-all-the-thumbs-curate/docs/rehype-all-the-thumbs-curate/",
    "coverage": "nyc report --reporter=text-lcov | codecov --pipe",
    "lint": "eslint src/index.js",
    "build": "mkdir -p dist/umd && npm run build:umd && npm run build:min",
    "build:umd": "esbuild src/index.js --bundle > dist/umd/index.js",
    "build:min": "esbuild src/index.js --bundle --minify > dist/index.min.js"
  },
  "dependencies": {
    "hast-util-select": "^4.0.0"
  },
  "devDependencies": {
    "codecov": "^3.8.0",
    "esbuild": "^0.8.0",
    "eslint": "^7.12.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-jsdoc3": "^3.0.0",
    "husky": "^4.3.0",
    "jsdoc": "^3.6.6",
    "minami": "^1.2.3",
    "np": "^7.0.0",
    "nyc": "^15.0.0",
    "prettier": "^2.1.2",
    "rehype-format": "^3.1.0",
    "rehype-parse": "^7.0.1",
    "rehype-remark": "^8.0.0",
    "rehype-stringify": "^8.0.0",
    "remark-lint": "^8.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "remark-stringify": "^9.0.0",
    "tap-nyc": "^1.0.3",
    "tape": "^5.0.1",
    "tape-promise": "^4.0.0",
    "to-vfile": "^6.1.0",
    "unified": "^9.2.0",
    "vfile": "^4.2.0",
    "vfile-reporter": "^6.0.1",
    "vfile-reporter-pretty": "^5.0.0"
  },
  "author": "Eric D Moore",
  "license": "MIT",
  "homepage": "https://github.com/ericdmoore/rehype-all-the-thumbs-curate#readme",
  "bugs": {
    "url": "https://github.com/ericdmoore/rehype-all-the-thumbs-curate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericdmoore/rehype-all-the-thumbs-curate.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run _docs",
      "pre-push": "npm test"
    }
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 90
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "directories": {
    "lib": "src",
    "doc": "docs",
    "test": "tests"
  },
  "keywords": [
    "unified",
    "unifiedjs",
    "rehype",
    "rehypejs",
    "rehype-plugin",
    "plugin",
    "thumbnails",
    "picture",
    "responsive",
    "images",
    "html",
    "tree"
  ]
}
