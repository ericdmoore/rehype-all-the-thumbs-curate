{
  "name": "rehype-all-the-thumbs-curate",
  "version": "0.3.0",
  "description": "Supporting rehype-all-the-thumbs by finding elements to be processed",
  "main": "src/index.js",
  "files": [
    "dist/umd/index.js",
    "dist/index.min.js"
  ],
  "scripts": {
    "test": "tape tests/**/*.test.js",
    "tests": "node tests/index.test.js",
    "test:NYC": "nyc npm test | tap-nyc",
    "genDocs": "typedoc src/index.ts",
    "openDocs":"open docs/index.html",
    "coverage": "nyc report --reporter=text-lcov | codecov --pipe",
    "lint": "eslint src/index.js",
    "build": "tsc",
    "esbuild": "mkdir -p dist/umd && npm run esbuild:umd && npm run esbuild:min",
    "esbuild:umd": "esbuild src/index.ts --bundle --platform=node > dist/umd/index.js",
    "esbuild:min": "esbuild src/index.ts --bundle --platform=node --minify > dist/index.min.js"
  },
  "dependencies": {
    "hast-util-select": "^4.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/mustache": "^4.0.1",
    "@types/sharp": "^0.26.0",
    "@types/vfile": "^4.0.0",
    "codecov": "^3.8.0",
    "esbuild": "^0.8.0",
    "eslint": "^7.12.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-jsdoc3": "^3.0.0",
    "hastscript": "^6.0.0",
    "husky": "^4.3.0",
    "jsdoc": "^3.6.6",
    "json-diff": "^0.5.4",
    "lodash.get": "^4.4.2",
    "minami": "^1.2.3",
    "mustache": "^4.0.1",
    "np": "^7.0.0",
    "nyc": "^15.0.0",
    "prettier": "^2.1.2",
    "rehype-format": "^3.1.0",
    "rehype-parse": "^7.0.1",
    "rehype-remark": "^8.0.0",
    "rehype-stringify": "^8.0.0",
    "remark-lint": "^8.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "remark-stringify": "^9.0.0",
    "source-map-support": "^0.5.19",
    "tap-nyc": "^1.0.3",
    "tape": "^5.0.1",
    "tape-promise": "^4.0.0",
    "to-vfile": "^6.1.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.28",
    "typescript": "^4.2.2",
    "unified": "^9.2.0",
    "vfile": "^4.2.0",
    "vfile-reporter": "^6.0.1",
    "vfile-reporter-pretty": "^5.0.0"
  },
  "author": "Eric D Moore",
  "license": "MIT",
  "homepage": "https://github.com/ericdmoore/rehype-all-the-thumbs-curate#readme",
  "bugs": {
    "url": "https://github.com/ericdmoore/rehype-all-the-thumbs-curate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericdmoore/rehype-all-the-thumbs-curate.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run genDocs",
      "pre-push": "npm test"
    }
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 90
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "directories": {
    "lib": "src",
    "doc": "docs",
    "test": "tests"
  },
  "keywords": [
    "unified",
    "unifiedjs",
    "rehype",
    "rehypejs",
    "rehype-plugin",
    "plugin",
    "thumbnails",
    "picture",
    "responsive",
    "images",
    "html",
    "tree"
  ]
}
