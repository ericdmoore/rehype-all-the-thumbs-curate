{
  "name": "rehype-all-the-thumbs-curate",
  "version": "0.1.0",
  "description": "Supporting [`rehype-all-the-thumbs`](https://github.com/ericdmoore/rehype-all-the-thumbs) by finding the elements to be processed",
  "main": "src/index.js",
  "author": "Eric D Moore",
  "license": "MIT",
  "homepage": "https://github.com/ericdmoore/rehype-all-the-thumbs-curate#readme",
  "bugs": {
    "url": "https://github.com/ericdmoore/rehype-all-the-thumbs-curate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericdmoore/rehype-all-the-thumbs-curate.git"
  },
  "scripts": {
    "test": "tape tests/**/*.test.js",
    "test:NYC": "nyc npm test",
    "docs": "gulp doc && open docs/rehype-all-the-thumbs-curate/0.1.0/index.html",
    "_docs": "jsdoc src/index.js -c .jsdoc.json && open docs/rehype-all-the-thumbs-curate/0.1.0/index.html",
    "covEmit": "nyc report --reporter=text-lcov | codecov -t ${CODECOV_TOKEN} --pipe",
    "coverage": "npm run covEmit"
  },
  "dependencies": {
    "hast-util-select": "^4.0.0",
    "json5": "^2.1.3"
  },
  "devDependencies": {
    "codecov": "^3.8.0",
    "eslint": "^7.12.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-jsdoc3": "^3.0.0",
    "husky": "^4.3.0",
    "jsdoc": "^3.6.6",
    "minami": "^1.2.3",
    "np": "^7.0.0",
    "nyc": "^15.0.0",
    "prettier": "^2.1.2",
    "rehype-format": "^3.1.0",
    "rehype-parse": "^7.0.1",
    "rehype-remark": "^8.0.0",
    "rehype-stringify": "^8.0.0",
    "remark-lint": "^8.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "remark-stringify": "^9.0.0",
    "tap-nyc": "^1.0.3",
    "tape": "^5.0.1",
    "tape-promise": "^4.0.0",
    "to-vfile": "^6.1.0",
    "unified": "^9.2.0",
    "vfile": "^4.2.0",
    "vfile-reporter": "^6.0.1",
    "vfile-reporter-pretty": "^5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run _docs",
      "pre-push": "npm test"
    }
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 90
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "directories": {
    "lib": "src",
    "doc": "docs",
    "test": "tests"
  },
  "keywords": [
    "unified",
    "unifiedjs",
    "rehype",
    "rehypejs",
    "rehype-plugin",
    "plugin",
    "thumbnails",
    "picture",
    "responsive",
    "images",
    "html",
    "tree"
  ]
}
