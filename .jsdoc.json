{
    "plugins": ["plugins/markdown"],
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["src", "package.json", "README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "templates": {
        "showInheritedInNav": true,
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "cleverLinks": false
    },
    "opts": {
        "template": "./node_modules/minami",
        "destination": "./docs/",
        "encoding": "utf8",
        "private": true,
        "recurse": true
    }
}